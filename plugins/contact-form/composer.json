{
    "name": "mojahid/contact-form",
    "type": "mojar-plugin",
    "license": "MIT",
    "description": "This plugin Developed For Maintain Contact Form And Send Email",
    "extra": {
        "mojar": {
            "name": "Contact Form",
            "domain": "contact_form",
            "cms_min": "1.0.0",
            "version": "1.0.0",
            "providers": [
                "Mojahid\\ContactForm\\Providers\\ContactFormServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Mojahid\\ContactForm\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
         "juzaweb/modules": "^1.0",
         "juzaweb/dev-tool": "^1.0"
    }
}
