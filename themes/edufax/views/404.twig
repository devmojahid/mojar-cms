{% extends 'cms::layouts.frontend' %}

{% block content %}
     <section class="tf__404_page">
        <div class="container">
            <div class="row">
                <div class="col-xl-6 col-md-8 m-auto wow fadeInUp" data-wow-duration="1s">
                    <div class="tf__404_text">
                        <div class="tf__404_img">
                            <img src="{{ asset('images/404.jpg') }}" alt="error" class="img-fluid w-100">
                        </div>
                        <h2>{{ __('Oops! Page not found.') }}</h2>
                        <p>{{ __('The page you are looking for might have been removed had its name changed or is temporarily unavailable.') }}</p>
                        <a class="tf__common_btn" href="{{ home_url() }}">{{ __('Back To Home') }}</a>
                    </div>
                </div>
            </div>
        </div>
    </section>
{% endblock %}

